{
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": [
            "last 2 versions",
            "safari >= 7"
          ]
        }
      ],
      ["@babel/preset-react",{
        "runtime": "automatic"
      }]
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        { "corejs": 2 }
      ]
    ]
  }
  